    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-node": "^30.0.4",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
